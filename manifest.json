{
  "manifest_version": 3,
  "name": "Close Duplicate Tabs",
  "version": "1.0",
  "description": "We close tabs with one clicks",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "bg.js",
    "type": "module"
  },
  "permissions": [
    "tabGroups",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  }  
}
